<%- include('partials/header'); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="publishedTitle">Title</label>
        <input type="text" class="form-control" name="publishedTitle" id="publishedTitle" autocomplete="off">
    </div>
    <div class="form-group">
        <label for="publishedArticle">Post</label>
        <textarea name="publishedArticle" class="form-control" id="publishedArticle" rows="8" cols="80" autocomplete="off"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>
<%- include('partials/footer'); -%>